{
  "name": "vscode-pdf",
  "displayName": "PDF Viewer",
  "description": "Portable document format (PDF) viewer for Visual Studio Code.",
  "version": "0.0.6",
  "publisher": "mathematic",
  "engines": {
    "vscode": "^1.58.1"
  },
  "icon": "icon.png",
  "license": "Apache-2.0",
  "keywords": [
    "pdf",
    "viewer"
  ],
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:pdf.view"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "pdf.view",
        "displayName": "PDF View",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "build": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "yarn run compile",
    "test-watch": "yarn run watch"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "1.81.0",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {}
}
